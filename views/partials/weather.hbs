{{#section 'weather'}}
<link rel="stylesheet" href="/css/weather.css">
{{/section}}

<div class="weather">
	{{#each partials.weatherContext.locations}}
	<div class="weather-img">
		<img src="" alt="天气" class="weather-i">
	</div>
	<div class="weather-info">
		<p class="weather-f">{{weather}}</p>
		<p class="weather-t">{{temp}}</p>
	</div>
	{{/each}}
</div>